{
  "name": "cal-todo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild": "npm run clear",
    "build": "next build",
    "prestart": "npm run build",
    "start": "npx serve out",
    "clear": "rm -rf node_modules/.cache",
    "test:unit": "jest --watch",
    "test:unit:ci": "jest --ci",
    "test:lint": "next lint",
    "test:lint:fix": "next lint --fix",
    "test:types": "tsc --pretty --noEmit",
    "test:serve": "start-server-and-test test:serve:start http://localhost:5000 test:serve:verify",
    "test:serve:start": "npm run start -- -l 5000",
    "test:serve:verify": "echo 'ok'",
    "test:ci": "CI=true run-s test:lint test:types test:unit:ci test:serve"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.0",
    "@chakra-ui/next-js": "^2.1.5",
    "@chakra-ui/react": "^2.8.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@tanstack/react-query": "^4.33.0",
    "@types/node": "20.5.2",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "classnames": "^2.3.2",
    "date-fns": "^2.30.0",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.19",
    "framer-motion": "^10.16.1",
    "lodash": "^4.17.21",
    "next": "13.4.19",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.1.6"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.0.1",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.3",
    "jest-environment-jsdom": "^29.6.3",
    "npm-run-all2": "^6.0.6",
    "prettier": "^3.0.2",
    "start-server-and-test": "^2.0.0"
  }
}
